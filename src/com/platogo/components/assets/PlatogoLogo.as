package com.platogo.components.assets {	import flash.net.URLRequest;		import flash.display.Sprite;		import flash.events.MouseEvent;import flash.net.navigateToURL;			/**	 * @author sirflo	 */	public class PlatogoLogo extends Sprite	{		public function PlatogoLogo()		{			addEventListener( MouseEvent.CLICK, clickListener, false, 0, true );		}			// ============================================================================================				protected function clickListener( event : MouseEvent ) : void		{			navigateToURL( new URLRequest( "http://www.platogo.com" ), "_blank" );		}	}}